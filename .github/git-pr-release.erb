Release <%= Time.now.strftime('%Y-%m-%d %H:%M') %>
> [!CAUTION]
> **Create Merge Commit** is recommended to merge this PR.
<% enhancement_prs = pull_requests.select { |pr| pr.labels.include?('enhancement') } %>
<% if enhancement_prs.any? %>
## 🚀 Enhancement
<% enhancement_prs.each do |pr| -%>
<%= pr.to_checklist_item %>
<% end -%>
<% end %>
<% bug_prs = pull_requests.select { |pr| pr.labels.include?('bug') } %>
<% if bug_prs.any? %>
## 🐛 Bug Fixes
<% bug_prs.each do |pr| -%>
<%= pr.to_checklist_item %>
<% end -%>
<% end %>
<% documentation_prs = pull_requests.select { |pr| pr.labels.include?('documentation') } %>
<% if documentation_prs.any? %>
## 📚 Documentation
<% documentation_prs.each do |pr| -%>
<%= pr.to_checklist_item %>
<% end -%>
<% end %>
